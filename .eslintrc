{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "standard",
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/prefer-default-export": "off",
    "no-console": "off",
    "import/extensions": "off",
    "import/no-unresolved": "off",
    "no-unused-vars": ["error"]
  }
}
